<h1 align="center">Categorias</h1>
<%= will_paginate %>
<% @categorias.each do |categoria| %>
  <ul class="lista">
    <div class="row">
      <div class="well col-md-4 col-md-offset-4">
        <li class="titulo-artigo">
          <%= link_to categoria.nome, categoria_path(categoria) %>
        </li>
      </div>
    </div>
  </ul>
<% end %>
<%= will_paginate %>